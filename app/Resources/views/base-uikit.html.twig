<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8" />
  <title>{% block title %}Welcome!{% endblock %}</title>
  {% block stylesheets %}
    <link rel="stylesheet" href="{{ asset("vendor/uikit/css/uikit.css") }}"/>
    <link rel="stylesheet" href="{{ asset("vendor/uikit/css/uikit.almost-flat.css") }}"/>
    <link rel="stylesheet" href="{{ asset("css/styles.css") }}"/>
    <link rel="stylesheet" href="{{ asset("vendor/uikit/css/components/datepicker.css") }}"/>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
  {% endblock %}
</head>
<body>
{# top navbar. #}
<header class="uk-navbar rc-navbar">
  <div class="uk-container uk-container-center">
    <div class="uk-grid">
      <div class="uk-width-1-3">
        <span>Rocket.mx</span>
      </div>
      <div class="uk-width-1-3">
        {% block header_menu %}{% endblock %}
      </div>
      <div class="uk-width-1-3">
        {% block user_menu %}{% endblock %}
      </div>
    </div>
  </div>
</header>
<div class="uk-container uk-container-center rc-content">
  <div class="uk-grid">
    <div class="uk-width-medium-2-10">
      {% block navigation %}
        {% include "navigation/navigation.html.twig" %}
      {% endblock %}
    </div>
    <div class="uk-width-medium-8-10">
      <h1 class="rc-page-title">{% block pagetitle %}{% endblock %}</h1>
      {% block content %}{% endblock %}
    </div>
  </div>
</div>

</div>
{% block javascripts %}
  <script src="{{ asset("vendor/jquery/jquery.min.js") }}"></script>
  <script src="{{ asset("vendor/uikit/js/uikit.js") }}"></script>
  <script src="{{ asset("vendor/uikit/js/components/datepicker.js") }}"></script>
{% endblock %}
</body>
</html>