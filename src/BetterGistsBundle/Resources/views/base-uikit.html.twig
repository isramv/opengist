<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8" />
  <title>{% block title %}Welcome!{% endblock %}</title>
  {% block stylesheets %}
    <link rel="stylesheet" href="{{ asset("src/uikit/css/uikit.css") }}"/>
    <link rel="stylesheet" href="{{ asset("src/uikit/css/uikit.almost-flat.css") }}"/>
    <link rel="stylesheet" href="{{ asset("src/css/styles.css") }}"/>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
  {% endblock %}
</head>
<body>
<div class="uk-container uk-container-center">
  <div class="rc-block">
    {% block header %}{% endblock %}
    {% block content %}{% endblock %}
    {% block body %}{% endblock %}
    {% block footer %}{% endblock %}
  </div>
</div>

</div>
{% block javascripts %}
  <script src="{{ asset("src/js/vendor/jquery/jquery.min.js") }}"></script>
  <script src="{{ asset("src/uikit/js/uikit.js") }}"></script>
{% endblock %}
</body>
</html>